// ==UserScript==
// @name           twitter filter
// @namespace      http://twitter.com/
// @include        http://twitter.com/
// ==/UserScript==

regexes = [ /#\S*{10,}/ ];

function filter(node) {
    for (i in queries) {
        regexes.match(node.nodeValue);
    }
}

HTMLElement.__proto__ = function() {
    
}
